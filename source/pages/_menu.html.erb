<section id="menu-section" class="page">
  <div class="container">
    <h2 class="page-title"><%= t('menu-section.title') %></h2>
    <hr class="title-sep" />
    <div class="menu-section-flex-container">
      <div id="slide-container">
        <div id="slide-element-1">
          <%= image_tag "data:image/png;base64,", data: { src: "https://res.cloudinary.com/dpra9vwkq/image/upload/v1597238020/tuna_carousel.jpg" }, alt: "N21 Restaurant - High Standard Bistronomie", class: "slide-image lazy", width: "650", height: "366" %>
        </div>
        <div id="slide-element-2">
          <%= image_tag "data:image/png;base64,", data: { src: "https://res.cloudinary.com/dpra9vwkq/image/upload/v1600174850/carousel_dessert.jpg" }, alt: "N21 Restaurant - High Standard Bistronomie", class: "slide-image lazy", width: "650", height: "366" %>
        </div>
        <div id="slide-element-3">
          <%= image_tag "data:image/png;base64,", data: { src: "https://res.cloudinary.com/dpra9vwkq/image/upload/v1597237652/brulee_carousel.jpg" }, alt: "N21 Restaurant - High Standard Bistronomie", class: "slide-image lazy", width: "650", height: "366" %>
        </div>
        <div id="slide-element-4">
          <%= image_tag "data:image/png;base64,", data: { src: "https://res.cloudinary.com/dpra9vwkq/image/upload/v1597238020/lamb_carousel.jpg" }, alt: "N21 Restaurant - High Standard Bistronomie", class: "slide-image lazy", width: "650", height: "366" %>
        </div>
      </div>
      <div class="menu-text-container">
        <p><%= t('menu-section.text_html') %></p>
        <div class="menu-link-wrapper">
          <a id="menu-link" href="https://res.cloudinary.com/dpra9vwkq/image/upload/v1597238981/N21_restaurant_menu.pdf" target="_blank" rel="noopener noreferrer"><%= t('menu-section.menu-link') %></a>
        </div>
      </div>
    </div>
  </div>
  <div class="order_booking container">
    <div class="cta-buttons">
      <button type="button" class="order-btn" data-glf-cuid="dd89f236-fb7a-448c-a3a9-77820e9d8f7d" data-glf-ruid="978c69b9-8004-465e-968d-c01b6e884c1b" ><%= t('nav.order-link')%></button>
      <button  type="button" class="order-btn reservation" data-glf-cuid="dd89f236-fb7a-448c-a3a9-77820e9d8f7d" data-glf-ruid="978c69b9-8004-465e-968d-c01b6e884c1b" data-glf-reservation="true"><%= t('nav.resa-link') %></button>
      <script src="https://www.fbgcdn.com/embedder/js/ewm2.js" defer async ></script>
    </div>
  </div>
  <section id="gallery-section">
    <h2 class="page-title"><%= t('gallery-section.title') %></h2>
    <hr class="title-sep" />
    <div class="gallery container">
      <% data.gallery.each do |owner, pic| %>
      <div class="brick">
        <a href="<%= pic.url %>" data-lightbox="picture-gallery">
          <%= image_tag "data:image/png;base64,", data: { src:  pic.url }, alt: "N21 Restaurant - High Standard Bistronomie", class: "lazy gallery-img" %>
        </a>
      </div>
      <% end %>
    </div>
  </section>
</section>
<section class="event-section">
  <div class="container">
    <div class="events-wrapper">
      <h2 class="page-title"><%= t('events-section.title') %></h2>
      <hr class="title-sep" />
      <div class="events-text">
        <p><%= t('events-section.text1_html') %></p>
        <p><%= t('events-section.text2_html') %></p>
        <p><%= t('events-section.text3_html') %></p>
        <p><%= t('events-section.link-start') %> <%= link_to t('events-section.link-text'), "#contact-section", id: "events-link"%> <%= t('events-section.link-end') %></p>
      </div>
      <div class="gallery container">
        <% data.event.each do |owner, pic| %>
        <div class="brick">
          <a href="<%= pic.url %>" data-lightbox="picture-gallery-events">
            <%= image_tag "data:image/png;base64,", data: { src:  pic.url }, alt: "N21 Restaurant - High Standard Bistronomie", class: "lazy" %>
          </a>
        </div>
        <% end %>
      </div>
      <div class="clients">
        <h2><%= t('events-section.clients') %></h2>
        <hr class="title-sep" />
        <ul class="brands">
          <% data.clients.each do |owner, client| %>
            <li class="brands__item">
              <%= image_tag client.url, alt: "N21 Restaurant - Events clients", class: "clients-logo lazy", loading: "lazy" %>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</section>
